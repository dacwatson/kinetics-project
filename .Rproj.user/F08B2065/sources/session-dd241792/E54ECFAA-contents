# sd tests

`4 aSf 30 PrLDm` %>%
  group_by(hours) %>%
  summarise(
    `mean` = mean(`030_139`, `030_143`, `030_145`, `030_148`, na.rm=TRUE),
    `sd` = sd(`030_139`, `030_143`, `030_145`, `030_148`, na.rm=TRUE)
    )



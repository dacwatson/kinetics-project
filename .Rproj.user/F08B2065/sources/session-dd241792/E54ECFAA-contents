# make hours consistent for exp 030-139 onwards

X030_139_hr <- X030_139 %>% mutate(across(hours, round, 3))
X030_143_hr <- X030_143 %>% mutate(across(hours, round, 3))
X030_145_hr <- X030_145 %>% mutate(across(hours, round, 3))
X030_148_hr <- X030_148 %>% mutate(across(hours, round, 3))
X030_149_hr <- X030_149 %>% mutate(across(hours, round, 3))


library("hablar")

X030_139_filtered <- filter(
  X030_139_hr, X030_139_hr$hours %% (round(1/3,3)) < 0.1)
X030_139_filtered_length <- as_tibble(lapply(X030_139_filtered, `length<-`, max(lengths(X030_145))))
X030_139_flt_ln <- X030_139_filtered_length %>% retype()

#write data to file for origin

write.csv(X030_139_flt_ln,file="data\\renamed columns\\030_139.csv")
write.csv(X030_148_hr,file="data\\renamed columns\\030_148.csv")
write.csv(X030_149_hr,file="data\\renamed columns\\030_149.csv")


